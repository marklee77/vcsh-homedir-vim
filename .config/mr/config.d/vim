[${HOME}/.vim/bundle/querycommandcomplete.vim]
checkout = git clone https://gitlab.stillwell.me/mark/querycommandcomplete.vim.git querycommandcomplete.vim
           git remote set-url --add --push git@stillwell.me:mark/querycommandcomplete.vim.git
           git remote set-url --add --push git@github.com:marklee77/querycommandcomplete.vim.git
           git remote set-url --add --push git@bitbucket.org:marklee77/querycommandcomplete.vim.git

