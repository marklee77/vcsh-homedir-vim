[${XDG_CONFIG_HOME:-$HOME/.config}/vcsh/repo.d/vim.git]
checkout = vcsh clone https://github.com/marklee77/vcsh-homedir-vim.git vim
commit   = vcsh vim commit -a
update   = vcsh vim pull
push     = vcsh vim push git@github.com:marklee77/vcsh-homedir-vim.git 
status   = vcsh vim status
gc       = vcsh vim gc


[.vim/bundle/querycommandcomplete.vim]
checkout = git clone https://github.com/marklee77/querycommandcomplete.vim.git querycommandcomplete.vim
push     = git push git@github.com:marklee77/querycommandcomplete.vim.git 

[.vim/bundle/mailtools.vim]
checkout = git clone https://github.com/marklee77/mailtools.vim.git mailtools.vim
push     = git push git@github.com:marklee77/mailtools.vim.git  

